<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity ItemStock.
    -->
    <changeSet id="20200903133023-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="item_stock_id"
                                 baseTableName="cims_item_stock_item_transaction"
                                 constraintName="fk_cims_item_stock_item_transaction_cims_item_stock_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cims_item_stock"/>

        <addForeignKeyConstraint baseColumnNames="item_transaction_id"
                                 baseTableName="cims_item_stock_item_transaction"
                                 constraintName="fk_cims_item_stock_item_transaction_item_transaction_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cims_item_transaction"/>
        
        <addForeignKeyConstraint baseColumnNames="inv_storage_id"
                                 baseTableName="cims_item_stock"
                                 constraintName="fk_cims_item_stock_inv_storage_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cims_inv_storage"/>

        <addForeignKeyConstraint baseColumnNames="storage_unit_id"
                                 baseTableName="cims_item_stock"
                                 constraintName="fk_cims_item_stock_storage_unit_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cims_meas_units"/>

        <addForeignKeyConstraint baseColumnNames="item_id"
                                 baseTableName="cims_item_stock"
                                 constraintName="fk_cims_item_stock_item_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cims_inv_item"/>

    </changeSet>
</databaseChangeLog>
